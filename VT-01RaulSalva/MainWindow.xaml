<Window x:Class="VT_01RaulSalva.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VT_01RaulSalva"
        mc:Ignorable="d"
        Title="Gestion de usuarios" Height="450" Width="600">
    <StackPanel Margin="5">
        <StackPanel.Resources>
            <Style TargetType="Button" BasedOn="{StaticResource buttonStyle1}"/>
        </StackPanel.Resources>
        <StackPanel Orientation="Horizontal" Margin="10" HorizontalAlignment="Center">
            <Button Content="PRINCIPAL" Click="Button_Click"/>
            <Button Content="NUEVO" Click="Button_Click_1"/>
            <Button Content="BUSCAR" Click="Button_Click_2"/>
            <Button Content="SALIR" Click="Button_Click_3" />
        </StackPanel>

        <Frame x:Name="myFrame" Margin="5" NavigationUIVisibility="Hidden" Width="570" Height="330"/>

    </StackPanel>
</Window>
